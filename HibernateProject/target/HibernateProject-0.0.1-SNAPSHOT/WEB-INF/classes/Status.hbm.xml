<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="entities.Status" table="status">
      
      <id name="idStatus" type="int" column="idStatus">
         <generator class="native"/>
      </id>
      <property name="statusType" column="statusType" type="string"/>
   </class>
</hibernate-mapping>