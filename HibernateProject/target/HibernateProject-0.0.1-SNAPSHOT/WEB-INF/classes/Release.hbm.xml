<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
   <class name="entities.Release" table="release">
      
      <id name="idRelease" type="int" column="idRelease">
         <generator class="native"/>
      </id>
      <property name="date" column="date" type="string"/>
      <property name="version" column="version" type="string"/>
      <property name="project" column="project" type="int"/>
   
   </class>
</hibernate-mapping>