<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="entities.Iesiri" table="iesiri">
      
      <id name="idRelease" type="int" column="idRelease">
         <generator class="native"/>
      </id>
      <property name="data" column="data" type="date"/>
      <property name="versiune" column="versiune" type="string"/>
      <property name="project" column="project" type="int"/>
   
   </class>
</hibernate-mapping>