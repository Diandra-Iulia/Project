<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="entities.Priority" table="priority">
     
      <id name="idPriority" type="int" column="idPriority">
         <generator class="native"/>
      </id>
      <property name="priorityType" column="priorityType" type="string"/>
      
   </class>
</hibernate-mapping>