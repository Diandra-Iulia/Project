<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="entities.Project" table="project">
     
      <id name="idProject" type="int" column="idProject">
         <generator class="native"/>
      </id>
      <property name="projectTitle" column="projectTitle" type="string"/>
      <property name="description" column="description" type="string"/>
      <property name="projectManager" column="projectManager" type="int"/>
      
   </class>
</hibernate-mapping>